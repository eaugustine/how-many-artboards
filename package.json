{
  "name": "How Many Artboards?",
  "description": "A Sketch plugin for counting the number of artboards in your file or on your page.",
  "version": "2.0.2",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "How Many Artboards?",
    "manifest": "src/manifest.json",
    "main": "How Many Artboards?.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.8.0"
  },
  "author": "Erica Augustine <eaugustine@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eaugustine/how-many-artboards.git"
  }
}
